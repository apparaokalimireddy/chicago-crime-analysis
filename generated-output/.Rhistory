base_url<-"http://www.houstontx.gov/police/cs/xls/NNNNN.xls"
years<-c(12, 13, 14, 15, 16, 17)
months<-c("jan","feb","mar","apr","may","jun","jul","aug","sep","oct","nov","dec")
first<-TRUE
for(y in years) {
for(m in months) {
if (y == '17' && m == 'sep')
break;
url<-gsub("NNNNN", paste(m,y,sep=""), base_url,fixed=TRUE)
df<-read.xls(url)
if (first) {
write.csv(df, file="houston_crimes.csv", row.names=FALSE, col.names=TRUE,append=FALSE)
first<-false
} else {
write.csv(df, file="houston_crimes.csv", row.names=FALSE, col.names=FALSE,append=TRUE)
}
}
}
